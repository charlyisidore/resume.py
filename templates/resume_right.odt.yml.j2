masterstyles:
  - class: style.MasterPage
    attrs:
      name: Standard
      pagelayoutname: cv_pagelayout

automaticstyles:
  - class: style.PageLayout
    attrs:
      name: cv_pagelayout
    children:
      - class: style.PageLayoutProperties
        attrs:
          pagewidth: 210mm
          pageheight: 297mm
          margintop: 10mm
          marginright: 5mm
          marginbottom: 10mm
          marginleft: 5mm
          printorientation: portrait
        children:
          - class: style.Columns
            attrs:
              columncount: 2
            children:
              - class: style.ColumnSep
                attrs:
                  width: 0.01cm
                  color: "#dddddd"
                  height: 100%
                  style: solid
              - class: style.Column
                attrs:
                  relwidth: "3345*"
                  startindent: 0cm
                  endindent: 0.25cm
              - class: style.Column
                attrs:
                  relwidth: "7995*"
                  startindent: 0.25cm
                  endindent: 0cm
  - class: text.ListStyle
    attrs:
      name: cv_list
    children:
      - class: text.ListLevelStyleBullet
        attrs:
          level: 1
          stylename: cv_bullet
          bulletchar: "-"
        children:
          - class: style.ListLevelProperties
            attrs:
              listlevelpositionandspacemode: label-alignment
            children:
              - class: style.ListLevelLabelAlignment
                attrs:
                  labelfollowedby: listtab
                  listtabstopposition: 3mm
                  textindent: -2mm
                  marginleft: 3mm

