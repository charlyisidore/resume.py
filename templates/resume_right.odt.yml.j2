              stylename: cv_skilllabel
              text: {{ item.label | tojson }}
{% elif item.level %}
          - class: text.Tab
{% endif %}
{% if item.level %}
          - class: text.Tab
{% for i in range(item.level) %}
          - class: text.Span
            attrs:
              stylename: cv_skilllevel
              text: "⬤"
{% endfor %}
{% for i in range(5-item.level) %}
          - class: text.Span
            attrs:
              stylename: cv_skilllevel
              text: "◯"
{% endfor %}
{% endif %}
{% if item.keywords %}
      - class: text.List
