text:

{% if contact %}
  - class: text.Section
    attrs:
      name: Contact
      stylename: cv_section
    children:
      - class: text.H
        attrs:
          outlinelevel: 1
          stylename: cv_sectiontitle_break
          text: Contact
{% if contact.email %}
{% for email in contact.email | aslist %}
      - class: text.P
        attrs:
          stylename: cv_paragraph
        children:
          - class: text.Span
            attrs:
              stylename: cv_icon
              text: "🖂"
          - class: text.S
            attrs:
              c: 2
          - class: text.Span
            attrs:
              stylename: cv_text
              text: {{ email | tojson }}
{% endfor %}
{% endif %}
{% if contact.website %}
{% for website in contact.website | aslist %}
      - class: text.P
        attrs:
          stylename: cv_paragraph
        children:
          - class: text.Span
            attrs:
              stylename: cv_icon
              text: "🌐"
          - class: text.S
            attrs:
              c: 2
          - class: text.Span
            attrs:
              stylename: cv_text
              text: {{ website | tojson }}
{% endfor %}
{% endif %}
{% if contact.telephone %}
{% for telephone in contact.telephone | aslist %}
      - class: text.P
        attrs:
          stylename: cv_paragraph
        children:
          - class: text.Span
            attrs:
              stylename: cv_icon
              text: "📞"
          - class: text.S
            attrs:
              c: 2
          - class: text.Span
            attrs:
              stylename: cv_text
              text: {{ telephone | tojson }}
{% endfor %}
{% endif %}
{% if False and contact.location %}
{% for location in contact.location | aslist %}
      - class: text.P
        attrs:
          stylename: cv_paragraph
        children:
          - class: text.Span
            attrs:
              stylename: cv_icon
              text: "📍"
          - class: text.S
            attrs:
              c: 2
          - class: text.Span
            attrs:
              stylename: cv_text
              text: {{ (location.city+", "+location.country) | tojson }}
{% endfor %}
{% endif %}
{% endif %}

{% if skills %}
  - class: text.Section
    attrs:
      name: Skills
      stylename: cv_section
    children:
      - class: text.H
        attrs:
          outlinelevel: 1
          stylename: cv_sectiontitle
          text: Skills
{% for item in skills %}
      - class: text.P
        attrs:
          stylename: cv_skill
        children:
          - class: text.Span
            attrs:
              stylename: cv_skillname
              text: {{ item.name | tojson }}
{% if item.label %}
          - class: text.Tab
          - class: text.Span
            attrs:
              stylename: cv_skilllabel
              text: {{ item.label | tojson }}
{% elif item.level %}
          - class: text.Tab
{% endif %}
{% if item.level %}
          - class: text.Tab
{% for i in range(item.level) %}
          - class: text.Span
            attrs:
              stylename: cv_skilllevel
              text: "⬤"
{% endfor %}
{% for i in range(5-item.level) %}
          - class: text.Span
            attrs:
              stylename: cv_skilllevel
              text: "◯"
{% endfor %}
{% endif %}
{% if item.keywords %}
      - class: text.List
        attrs:
          stylename: cv_list
        children:
{% for keyword in item.keywords | aslist %}
          - class: text.ListItem
            children:
              - class: text.P
                attrs:
                  stylename: cv_paragraph
                  text: {{ keyword | tojson }}
{% endfor %}
{% endif %}
{% endfor %}
{% endif %}

{% if languages %}
  - class: text.Section
    attrs:
      name: Languages
      stylename: cv_section
    children:
