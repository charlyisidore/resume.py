{% if False and references %}
  - class: text.Section
    attrs:
      name: References
      stylename: cv_section
    children:
      - class: text.H
        attrs:
          outlinelevel: 1
          stylename: cv_sectiontitle
          text: References
      - class: text.List
        attrs:
          stylename: cv_list
        children:
{% for item in references | aslist %}
          - class: text.ListItem
            children:
              - class: text.P
                attrs:
                  stylename: cv_paragraph
                  text: {{ item.name | tojson }}
              - class: text.P
                attrs:
                  stylename: cv_paragraph
                children:
                  - class: text.S
                    attrs:
                      c: 4
                  - class: text.Span
                    attrs:
                      stylename: cv_email
                      text: {{ item.email | tojson }}
{% endfor %}
{% endif %}

  - class: text.H
    attrs:
      outlinelevel: 1
      stylename: cv_title
      text: {{ name | tojson }}
  - class: text.H
    attrs:
      outlinelevel: 1
      stylename: cv_headline
      text: {{ headline | tojson }}

{% if education %}
  - class: text.Section
    attrs:
      name: Education
      stylename: cv_section
    children:
      - class: text.H
        attrs:
          outlinelevel: 1
          stylename: cv_sectiontitle
          text: Education
{% for item in education %}
      - class: text.H
        attrs:
          outlinelevel: 3
          stylename: cv_heading
          text: {{ item.title | tojson }}
      - class: text.P
        attrs:
          stylename: cv_paragraph
        children:
          - class: text.Span
            attrs:
              stylename: cv_organisation
              text: {{ item.organisation.name | tojson }}
          - class: text.S
            attrs:
              c: 4
          - class: text.Span
            attrs:
              stylename: cv_daterange
              text: {{ item.period.from | asdatetime("%Y") }} - {% if item.period.to %}{{ item.period.to | asdatetime("%Y") }}{% else %}today{% endif %}
          - class: text.S
            attrs:
              c: 4
          - class: text.Span
            attrs:
              stylename: cv_country
              text: {{ item.organisation.country | tojson }}
{% if item.activities %}
